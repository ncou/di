{
  "name": "phapi/di",
  "description": "Phapi Dependency Injection Container",
  "keywords": [
    "dependency injection container", "di", "phapi"
  ],
  "version": "1.0.0",
  "minimum-stability": "stable",
  "license": "MIT",
  "homepage": "http://phapi.github.io",
  "authors": [
    {
      "name": "Peter Ahinko",
      "email": "peter@ahinko.se",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "phapi/contract": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*",
    "mockery/mockery": "0.9.*",
    "codeclimate/php-test-reporter": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Phapi\\Di\\": "src/Phapi/Di/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Phapi\\Tests\\Di\\": "tests/Phapi/Di/"
    }
  }
}